<h1>Listando faixas_de_desconto</h1>

<table>
  <tr>
    <th>De</th>
    <th>Ate</th>
    <th>Desconto permitido</th>
  </tr>

<% @faixas_de_desconto.each do |faixa_de_desconto| %>
  <tr>
    <td><%=h faixa_de_desconto.de %></td>
    <td><%=h faixa_de_desconto.ate %></td>
    <td><%=h faixa_de_desconto.desconto_permitido %></td>
    <td><%= link_to 'Mostrar', faixa_de_desconto %></td>
    <td><%= link_to 'Editar', edit_faixa_de_desconto_path(faixa_de_desconto) %></td>
    <td><%= link_to 'Excluir', faixa_de_desconto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nova Faixa de desconto', new_faixa_de_desconto_path %>
